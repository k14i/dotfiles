###############################
## Page/Cursor motion
###############################

keymap C-d NEXT_PAGE
keymap C-u PREV_PAGE

keymap RIGHT MOVE_RIGHT
keymap C-l MOVE_RIGHT
keymap l MOVE_RIGHT

keymap LEFT MOVE_LEFT
keymap C-h MOVE_LEFT
keymap h MOVE_LEFT

#keymap  MOVE_DOWN
#keymap  MOVE_UP
#keymap  MOVE_RIGHT1
#keymap  MOVE_LEFT1

keymap DOWN MOVE_DOWN1
keymap C-j MOVE_DOWN1
keymap j MOVE_DOWN1

keymap UP MOVE_UP1
keymap C-k MOVE_UP1
keymap k MOVE_UP1

keymap C-e DOWN
keymap C-y UP

#keymap  SHIFT_LEFT
#keymap  SHIFT_RIGHT
#keymap  LEFT
#keymap  RIGHT

keymap 0 LINE_BEGIN
keymap $ LINE_END
#keymap  CENTER_H
keymap zz CENTER_V

#keymap  GOTO_LINE
keymap gg BEGIN
keymap G END
keymap w NEXT_WORD
keymap b PREV_WORD

#keymap  LINK_BEGIN
#keymap  LINK_END

keymap C-n NEXT_LINK
keymap C-p PREV_LINK
keymap f MOVE_LIST_MENU

#keymap  NEXT_RIGHT
#keymap  NEXT_LEFT
#keymap  NEXT_DOWN
#keymap  NEXT_UP
#keymap  NEXT_RIGHT_DOWN
#keymap  NEXT_LEFT_UP

keymap C-o UNDO
keymap TAB REDO


